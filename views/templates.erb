<h1><%= @title %></h1>

<table border="0" cellpadding="0" cellspacing="10" class="list">
<% @templates.each_with_index do |template, i| %>
	<tr<%= ' class="alt"' if i%2 == 0 %>>
		<td><a href="/template/<%= template.id %>"><%= template.text %></a></td>
		<td><a href="/deletetemplate/<%= template.id %>">delete</a></td>
<% end %>
</table>